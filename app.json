{
  "expo": {
    "name": "temp_project",
    "slug": "temp_project",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "tempproject",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "single"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-asset",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "reactCompiler": true
    }
  }
}